{
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "global-require": 0,
    "no-console": 0,
    "quotes": ["error", "double"],
    "quote-props": ["error", "consistent"]
  },
  "globals": {
    "document": true,
    "localStorage": true,
    "fetch": true,
    "Headers": true,
    "window": true
  }
}
